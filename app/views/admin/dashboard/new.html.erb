<h1>Add User</h1>

<%= form_for @user, url: admin_dashboard_index_path do |f| %>
  <div>
    <%= f.label :email %>
    <%= f.text_field :email %>
  </div>

  <div>
    <%= f.label :password %>
    <%= f.password_field :password %>
  </div>

  <div>
    <%= f.label :admin %>
    <%= f.check_box :admin %>
  </div>

  <%= f.submit 'Create User' %>
<% end %>

<%= link_to 'Back to Users', admin_dashboard_index_path %>